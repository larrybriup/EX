<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.2.xsd">
	<bean name="address" class="spring.ex.eagle1.bean.Address" autowire="byName" >
		<property name="city" value="上海"></property>
		<property name="street" value="文化路"></property>
		<property name="country" value="中国"></property>
	</bean>
	<bean name="person" class="spring.ex.eagle1.bean.Person" autowire="byType">
		<property name="sNo" value="2"></property>
		<property name="name" value="三炮"></property>
		<property name="gender" value="true"></property>
		<property name="age" value="12"></property>
	</bean>

</beans>
           